{
    "name": "scherersoftware/cakephp-schema",
    "description": "Schema saving and loading from file for CakePHP 3.0",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "orm", "database", "schema", "migrations"],
    "homepage": "https://github.com/scherersoftware/cakephp-schema",
    "license": "MIT",
    "authors": [
        {
            "name": "Ladislav Gallay",
            "role": "Author"
        },
        {
            "name": "Robert Scherer",
            "role": "Author"
        }
    ],
    "support": {
        "issues": "https://github.com/scherersoftware/cakephp-schema/issues",
        "source": "https://github.com/scherersoftware/cakephp-schema"
    },
    "require": {
        "cakephp/cakephp": "^3.4",
        "riimu/kit-phpencoder": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "<6.0",
        "cakephp/bake": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Schema\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schema\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
